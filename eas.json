{
  "cli": {
    "version": ">= 3.7.2"
  },
  "build": {
    "preview-development":{
      "env":{
        "FIREBASE_ENV":"dev"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview-production": {
      "env":{
        "FIREBASE_ENV":"prod"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m1-medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
